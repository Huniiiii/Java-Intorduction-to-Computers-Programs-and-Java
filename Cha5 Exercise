package exercise;

public class TestMax {

	public static void main(String[] args) {
		int a = 3;
		int b =5;
		
		int c  = addTwo(a,b);
		System.out.println(c);

	}
	
	public static int addTwo(int x, int y){
		int result = x +y;
		return result;
		
	}
	}

}

package exercise;

public class overloading {

	public static void main(String[] args) {
		System.out.println(addTwo(3, 4));
		System.out.println(addTwo(3.2, 4.3));

	}
	
	public static int addTwo(int a, int b) {
		
		return a + b;
	}
	
	public static double addTwo(double a, double b) {
		return a + b;
	}
	
	public static double addTwo(int a, double b) {
		return a + b;
	}
	
	public static double addTwo(double a, int b) {
		return a + b;
	}

}

package exercise;

public class palindrome {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	public static int reverse(int number) {
		int reverse = 0;
		int digit;
		while(number != 0){
			digit = number % 10;
			reverse = reverse * 10 + digit;
			number = number / 10;

		}
		return reverse;
		
		
	}

}

package exercise;

public class prime {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}

public static boolean isPrime(int number) {
	for (int i = 2; i <= number / 2; i++) { 
		if (number % i == 0) {
			return false;
				 }
			 }
			return true;
		 }
}
